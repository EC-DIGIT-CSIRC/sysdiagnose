{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}",
            "cwd": "${workspaceFolder}/"
        },
        {
            "name": "Python Debugger: analyse.py list analysers",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/analyse.py",
            "args": "list analysers",
            "cwd": "${workspaceFolder}/"
        },
        {
            "name": "Python Debugger: analyse.py analyse apps 1",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/analyse.py",
            "args": "analyse apps 1",
            "cwd": "${workspaceFolder}/"
        },
        {
            "name": "Python Debugger: logarchive.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/parsers/logarchive.py",
            "args": "-i data/1/sysdiagnose_2023.08.25_15-14-14+0200_iPhone-OS_iPhone_21A5319a/system_logs.logarchive",
            "cwd": "${workspaceFolder}/"
        },
        {
            "name": "Python Debugger: parsing.py list parsers",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/parsing.py",
            "args": "list parsers",
            "cwd": "${workspaceFolder}/"
        },
        {
            "name": "Python Debugger: parsing.py parse itunesstore",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/parsing.py",
            "args": "parse itunesstore 1",
            "cwd": "${workspaceFolder}/"
        },
        {
            "name": "Python Debugger: parsing.py parse ps",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/parsing.py",
            "args": "parse ps 1",
            "cwd": "${workspaceFolder}/"
        },
        {
            "name": "Python Debugger: parsing.py parse crashlogs",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/parsing.py",
            "args": "parse crashlogs 1",
            "cwd": "${workspaceFolder}/"
        },
        {
            "name": "Python Debugger: parsing.py parse plists",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/parsing.py",
            "args": "parse plists 1",
            "cwd": "${workspaceFolder}/"
        },
        {
            "name": "Python Debugger: parsing.py parse wifinetworks",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/parsing.py",
            "args": "parse wifinetworks 1",
            "cwd": "${workspaceFolder}/"
        }
    ]
}